#include<stdio.h>
void reverse(int*arr,int n){
    int *start=arr;
    int *end=arr+n-1;
    while(start<end){
        int temp=*start;
        *start=*end;
        *end=temp;
        start++;
        end--;
    }
}
int main(){
    int arr[5]={1,2,3,4,5};
    int n=5;
    reverse(arr,n);
    printf("reversed array:");
    for(int i=0;i<n;i++){
        printf("%d",arr[i]);
    }
}
